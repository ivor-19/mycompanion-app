{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
       "android": {
        "buildType": "apk"  
      },
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "$EXPO_PUBLIC_GEMINI_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
       "android": {
        "buildType": "apk"  
      },
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "$EXPO_PUBLIC_GEMINI_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
       "android": {
        "buildType": "apk"  
      },
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "$EXPO_PUBLIC_GEMINI_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
